[run]
source = .
omit = 
    */tests/*
    */venv/*
    */migrations/*
    */scripts/*
    */__pycache__/*
    */site-packages/*
    app.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

[html]
directory = htmlcov


